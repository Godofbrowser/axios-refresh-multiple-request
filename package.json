{
  "name": "axios-multi-auth-interception-demo",
  "version": "0.0.1",
  "description": "A simple app to demostrate axios multiple request authentication with refresh token",
  "main": "src/script.js",
  "scripts": {
    "dev": "parcel index.html -p 3000 --open",
