    "token",
    "interceptors"
  ],
  "author": "godofbrowser",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "path-to-regexp": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "json-server": "^0.15.1",
    "parcel-bundler": "^1.12.3"
  },
  "babel": {
    "plugins": [
      "@babel/proposal-class-properties"
    ]
  }
